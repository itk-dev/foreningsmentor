{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "itk": {
            "type": "composer",
            "url": "https://packagist.itkdev.dk"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address_dawa": "^1.0",
        "drupal/adminimal_theme": "^1.3",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/bricks": "^1.6",
        "drupal/cer": "^4.0@alpha",
        "drupal/core": "~8.0",
        "drupal/eck": "^1.0@alpha",
        "drupal/field_group": "^1.0",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/itk-admin-links": "^1.0",
        "drupal/itk-user-pages-theme": "^1.0",
        "drupal/itk_cookie_message": "^1.0",
        "drupal/itk_media_entity": "^1.1",
        "drupal/login_redirect_per_role": "^1.2",
        "drupal/menu_item_role_access": "^1.0",
        "drupal/metatag": "^1.7",
        "drupal/node_view_permissions": "^1.2",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.3",
        "drupal/redirect": "^1.2",
        "drupal/system_status": "^2.8",
        "drupal/toolbar_visibility": "^1.2",
        "drupal/views_add_button": "^1.0@RC",
        "drupal/views_bulk_operations": "^2.4",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/youtube": "^1.0@beta",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/console": "^1.8",
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/bricks": {
                "Fix deprecation warning": "https://www.drupal.org/files/issues/2018-10-30/3008553-deprecated-eck-entity-variable-%238.patch"
            }
        }
    }
}
