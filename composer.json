{
    "name": "itk-dev/foreningsmentor",
    "description": "Website for foreningsmentor",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "require": {
        "php": "^8.3.0",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/adminimal_theme": "^1.7",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/bricks": "^2.1",
        "drupal/cer": "^5.0@beta",
        "drupal/content_fixtures": "^3.1",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-project-message": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/csv_serialization": "^4.0",
        "drupal/eck": "^2.0",
        "drupal/entity": "^1.2",
        "drupal/field_group": "^3.2",
        "drupal/field_permissions": "^1.3",
        "drupal/honeypot": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/jquery_ui_datepicker": "^2.0",
        "drupal/jquery_ui_slider": "^2.0",
        "drupal/jquery_ui_touch_punch": "^1.0",
        "drupal/login_redirect_per_role": "^1.8",
        "drupal/memcache": "^2.5",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/metatag": "^1.16",
        "drupal/node_view_permissions": "^1.6",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/symfony_mailer": "^1.4",
        "drupal/upgrade_status": "^4.0",
        "drupal/views_add_button": "^2.0",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_data_export": "^1.0",
        "drush/drush": "^12.5",
        "furf/jquery-ui-touch-punch": "dev-master",
        "itk-dev/itk_siteimprove": "^1.3",
        "itk-dev/web_accessibility_statement": "^1.0",
        "webflo/drupal-finder": "^1.2",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^10.1",
        "ergebnis/composer-normalize": "^2.44"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "type": "drupal-library",
                "version": "dev-master",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal-composer/drupal-scaffold": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patcher": {
            "force-reset": true
        },
        "patches": {}
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    }
}
